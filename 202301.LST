10 OPEN 1,8,2,"2023011.TXT,P,R"11 GOSUB 90020 INPUT#1,A$21 COUNT=COUNT+130 GOSUB 1000100 IF ST<64   GOTO 20110 CLOSE 1120 END900 SUM=0910 COUNT=0990 RETURN1000 REM ***1010 FIRST$=""1011 FOR I=1 TO LEN(A$)1012 GOSUB 12001013 IF ASC(C$) >= ASC("0") AND ASC(C$)<= ASC("9") THEN FIRST$=C$:GOTO 10201014 NEXTI1020 LAST$=""1021 FOR I=LEN(A$) TO 1 STEP -11022 GOSUB 12001023 IF ASC(C$) >= ASC("0") AND ASC(C$)<= ASC("9") THEN LAST$=C$:GOTO 10301024 NEXT I1025 IFLAST$<>"" GOTO10301030 REM *** CALCULATE TWO-DIGIT NUMBER ***1031 TDN = 10 * ( ASC(FIRST$) - ASC("0") ) + ( ASC(LAST$) - ASC("0"))1032 SUM = SUM + TDN1033 PRINT COUNT, TDN, SUM, A$1040 RETURN1200 IF MID$(A$,I,4)="zero"  THEN C$="0":RETURN1201 IF MID$(A$,I,3)="one"   THEN C$="1":RETURN1202 IF MID$(A$,I,3)="two"   THEN C$="2":RETURN1203 IF MID$(A$,I,5)="three" THEN C$="3":RETURN1204 IF MID$(A$,I,4)="four"  THEN C$="4":RETURN1205 IF MID$(A$,I,4)="five"  THEN C$="5":RETURN1206 IF MID$(A$,I,3)="six"   THEN C$="6":RETURN1207 IF MID$(A$,I,5)="seven" THEN C$="7":RETURN1208 IF MID$(A$,I,5)="eight" THEN C$="8":RETURN1209 IF MID$(A$,I,4)="nine"  THEN C$="9":RETURN1210 C$=MID$(A$,I,1)1220 RETURN